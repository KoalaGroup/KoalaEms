ROOTBASE   := /usr/local/root
ROOTSYS   := $(ROOTBASE)/alpha-dec-osf4.0_cxx
ROOTLIBS  := -L$(ROOTSYS)/lib -lNew -lBase -lCint -lClib -lCont -lFunc -lGraf \
