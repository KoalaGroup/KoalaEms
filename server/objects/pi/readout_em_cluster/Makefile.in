CONF = ../../../conf

srcdir = @srcdir@
VPATH = @srcdir@

include $(CONF)/rules.make
include $(CONF)/config.make

#cfiles = datains.c piops.c di_cluster.c di_stream.c di_opaque.c
cfiles = datains.c piops.c di_cluster.c di_opaque.c
ofiles = $(cfiles:.c=.o)
depfiles = $(cfiles)

lib = ../../libobj.a

all: $(lib)

$(lib): $(lib)($(ofiles))
	$(AR) cr $@ $(ofiles)
	$(RANLIB) $@

depend: xdepend

.PHONY: depend
