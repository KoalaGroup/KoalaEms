  use "chi_map.d"
  psect fbutil_a,0,0,0,0,0


*******************************************************************************

* void FBCLEAR()

FBCLEAR:
        movem.l a0,-(a7)
        move.l  #F_BASE,a0
        tst.b   FCLEAR(a0)
        movem.l (a7)+,a0
        rts

*******************************************************************************

* void FBPULSE()

FBPULSE:
        movem.l a0,-(a7)
        move.l  #F_BASE,a0
        tst.b   OUT(a0)
        movem.l (a7)+,a0
        rts

