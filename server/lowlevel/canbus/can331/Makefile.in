CONF = ../../../conf

srcdir = @srcdir@
VPATH = @srcdir@

include $(CONF)/rules.make
include $(CONF)/config.make

cfiles = can331.c
ofiles = $(cfiles:.c=.o)
depfiles = $(cfiles)

CFLAGS += $(CANFLAGS)

lib = ../../libll.a

all: $(lib)

$(lib): $(lib)($(ofiles))
	$(RANLIB) $@

depend: xdepend

.PHONY: depend

