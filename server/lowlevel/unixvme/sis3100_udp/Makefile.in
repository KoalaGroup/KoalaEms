CONF = ../../../conf

srcdir = @srcdir@
VPATH = @srcdir@
@SET_MAKE@

include $(CONF)/rules.make
include $(CONF)/config.make

cfiles = vme_sis_udp.c
ofiles = $(cfiles:.c=.o)
depfiles = $(cfiles)

CFLAGS += $(VMEFLAGS)

lib = ../../libll.a

all: $(lib)

$(lib): $(lib)($(ofiles))
	$(RANLIB) $@

depend: xdepend

.PHONY: depend
