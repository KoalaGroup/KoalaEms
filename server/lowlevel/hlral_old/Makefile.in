CONF = ../../conf

srcdir = @srcdir@
VPATH = @srcdir@
@SET_MAKE@

include $(CONF)/rules.make
include $(CONF)/config.make

cfiles = hlral.c
ofiles = $(cfiles:.c=.o)
depfiles = $(cfiles)

lib = ../libll.a

all: $(lib)

$(lib): $(lib)($(ofiles))
	$(RANLIB) $@

depend: xdepend

.PHONY: depend

