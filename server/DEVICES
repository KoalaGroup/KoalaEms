int get_mod_type(unsigned int idx)
{
if (memberlist)
    return modullist->entry[memberlist[idx]].modultype; 
else
    return modullist->entry[idx].modultype; 
}

modul_class get_mod_class(unsigned int idx)
{
if (memberlist)
    return modullist->entry[memberlist[idx]].modulclass; 
else
    return modullist->entry[idx].modulclass; 
}

int valid_module(unsigned int idx, Modulclass class, int accept_unspec);

