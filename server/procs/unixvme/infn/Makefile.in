# procs/unixvme/infn/Makefile.in
# $ZEL: Makefile.in,v 1.1 2009/10/06 09:23:54 trusov Exp $

CONF = ../../../conf

srcdir = @srcdir@
VPATH = @srcdir@

include $(CONF)/rules.make
include $(CONF)/config.make

cfiles = INFN_triggerboard.c
ofiles = $(cfiles:.c=.o)
depfiles = $(cfiles)

UNIXVMETYPES = $(shell echo $(unixvme)|tr '[:lower:]' '[:upper:]')
UNIXVMEFLAGS=$(foreach typ,$(UNIXVMETYPES),-DUNIXVME_$(typ) )
CFLAGS += $(UNIXVMEFLAGS)

lib = ../../libprocs.a

all: $(lib)

$(lib): $(lib)($(ofiles))
	$(RANLIB) $@

clean:
	-$(RM) *.o

depend: xdepend

.PHONY: depend

#############################################
