CONF = ../../conf

srcdir = @srcdir@
VPATH = @srcdir@

include $(CONF)/rules.make
include $(CONF)/config.make

cfiles = mmappeddata.c
ofiles = $(cfiles:.c=.o)
depfiles = $(cfiles)

lib = ../libprocs.a

all: $(lib)

$(lib): $(lib)($(ofiles))
	$(RANLIB) $@

clean:
	-$(RM) *.o

depend: xdepend

.PHONY: depend

