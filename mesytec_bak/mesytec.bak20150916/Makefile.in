CONF = ../../../conf

srcdir = @srcdir@
VPATH = @srcdir@

include $(CONF)/rules.make
include $(CONF)/config.make

cfiles = madc32.c mtdc32.c

ofiles = $(cfiles:.c=.o)
depfiles = $(cfiles)

UNIXVMETYPES = $(shell echo $(unixvme)|tr '[:lower:]' '[:upper:]')
UNIXVMEFLAGS=$(foreach typ,$(UNIXVMETYPES),-DUNIXVME_$(typ) )
CFLAGS += $(UNIXVMEFLAGS)

lib = ../../libprocs.a

all: $(lib)

$(lib): $(lib)($(ofiles))
	$(RANLIB) $@

clean:
	-$(RM) *.o

depend: xdepend

.PHONY: depend

