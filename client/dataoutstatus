getdataoutstatus 0
0x00000000 0x00000003 0x00000000 0x0043dd21 0x00013db2 0x02030000 0x00003a01
error      status     disabled   rest       errorcount code1      code2

error:
  OS9-errorcode

status:
  0: running
  1: neverstarted
  2: done
  3: error

disabled:
  0: enabled
  1: disabled

rest:
  restband in kByte

errorcount:
  errors

code1:
  0x|0|2|03|00|00
     | | |  |  |__byte 21
     | | |  |_____byte 20
     | | |________byte 19
     | |__________sense key
     |____________flags2

code2:
  0x0000|3a|01
         |  |__additional sense code
         |_____sense qualifier

byte 21:
  bit 0: write splice error overshot
  ...    write splice error blank
         physical end of tape
         reserved 21/3 (reinigen)
         reserved 21/4
         reserved 21/5
         reserved 21/6
         reserved 21/7

byte 20:
  bit 0: formatter error
         servo system error
         write error 1
         underrun error
         filemark error
         write protected
         tape mark detect error
         reserved 20/7

byte 19:
  bit 0: logical begin of tape
         tape not present
         tape motion error
         error counter overflow
         media error
         formatted buffer parity error
         SCSI bus parity error
         power fail

flags2:
  bit 0: reserved 2/4
         illegal length indicator
         end of medium
         filemark


key==0:
  code==0
    qualifier==0: nix
               1: filemark during read
               2: logical end of tape
               4: logical begin of tape
  code==0x3b:
    qualifier==1: physicalor logical begin of tape
key==2:
  code==4:
    qualifier==0: not ready, cause unknown or tape not present
               1: not ready, but in progress
               2: not ready, tape motion command required
               3: not ready, failed power-on self-test
  code==0x3a
    qualifier==0: not ready, tape not present
key==3:
  code==0:
    qualifier==0: undetermined medium error
  code==3:
    qualifier==2: excessive write errors
  code==9:
    qualifier==0: tracking error
  code==0xc:
    qualifier==0: hard write error
  code==0x11:
    qualifier==1: hard read error
  code==0x3b:
    qualifier==0: position error
               2: physical end of tape
key==4:
  code==0:
    qualifier==0: undetermined hardware error
  code==9:
    qualifier==0: track following error
  code==0x40:
    qualifier==0x80: DPATH error
               0x81: DPATH underrun
  code==0x44:
    qualifier==0: Internal failure
key==5:
  code==0x1a:
    qualifier==0: parameter list error in CDB
  code==0x20:
    qualifier==0: illegal operation code
  code==0x24:
    qualifier==0: invalid field in CDB
  code==0x25:
    qualifier==0: logical unit not supported
  code==0x26:
    qualifier==0: invalid field in parameter list
  code==0x3d:
    qualifier==0: invalid LUN
  code==0x81:
    qualifier==0: mode mismatch
  code==0x84:
    qualifier==0: ??? compression mode mismatch ???
key==6:
  code==0x28:
    qualifier==0: tape load has occured, media may have been changed
  code==0x29:
    qualifier==0: power-on reset, SCSI bus reset or device reset
  code==0x2a:
    qualifier==1: mode select parameters have been changed
  code==0x3f:
    qualifier==1: new microcode was loaded
  code==0x5a:
    qualifier==1: ??? eject button pressed ???
key==7:
  code==0x27:
    qualifier==0: tape is write protected
key==8:
  code==0:
    qualifier==5: end of data on read operation
key==0xb:
  code==0x45:
    qualifier==0: reselect failed
  code==0x47:
    qualifier==0: SCSI bus parity error during write
  code==0x48:
    qualifier==0: initiator detected error message during read
